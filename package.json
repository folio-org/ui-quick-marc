{
  "name": "@folio/quick-marc",
  "version": "4.0.2",
  "description": "Quick MARC editor",
  "main": "index.js",
  "repository": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.9.4"
  },
  "stripes": {
    "actsAs": [
      "plugin"
    ],
    "pluginType": "quick-marc",
    "displayName": "ui-quick-marc.meta.title",
    "okapiInterfaces": {
      "inventory": "10.0 11.0",
      "records-editor.records": "1.4 2.0"
    },
    "stripesDeps": [
      "@folio/stripes-acq-components"
    ],
    "permissionSets": [
      {
        "permissionName": "ui-quick-marc.quick-marc-editor.all",
        "displayName": "quickMARC: View, edit MARC bibliographic record",
        "subPermissions": [
          "records-editor.records.item.get",
          "records-editor.records.item.put",
          "inventory.instances.item.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-quick-marc.quick-marc-holdings-editor.all",
        "displayName": "quickMARC: View, edit MARC holdings record",
        "subPermissions": [
          "records-editor.records.item.get",
          "records-editor.records.item.put",
          "inventory.instances.item.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-quick-marc.quick-marc-holdings-editor.create",
        "displayName": "quickMARC: Create a new MARC holdings record",
        "subPermissions": [
          "records-editor.records.status.item.get",
          "records-editor.records.item.get",
          "records-editor.records.item.post",
          "inventory.instances.item.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-quick-marc.quick-marc-editor.duplicate",
        "displayName": "quickMARC: Derive new MARC bibliographic record",
        "subPermissions": [
          "records-editor.records.status.item.get",
          "records-editor.records.item.get",
          "records-editor.records.item.post",
          "inventory.instances.item.get"
        ],
        "visible": true
      }
    ]
  },
  "scripts": {
    "build": "yarn stripes build --output ./output",
    "lint": "eslint .",
    "start": "yarn stripes serve",
    "test": "jest --ci --coverage",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-quick-marc ./translations/ui-quick-marc/compiled"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.7.4",
    "@folio/eslint-config-stripes": "^5.0.0",
    "@folio/stripes": "^7.0.0",
    "@folio/stripes-cli": "^2.0.0",
    "@formatjs/cli": "^4.2.7",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^11.0.4",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^26.3.0",
    "core-js": "^3.6.1",
    "eslint": "^6.2.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jest": "^23.0.4",
    "faker": "^4.1.0",
    "history": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-css-modules": "^2.1.0",
    "jest-junit": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "@folio/stripes-acq-components": "^2.1.0",
    "final-form": "^4.18.2",
    "final-form-arrays": "^3.0.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.5.10",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.0",
    "react-final-form-listeners": "^1.0.2",
    "react-hot-loader": "^4.3.12",
    "react-router-prop-types": "^1.0.4",
    "uuid": "^3.0.1"
  },
  "peerDependencies": {
    "@folio/stripes": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  }
}
