{
  "name": "@folio/quick-marc",
  "version": "1.0.0",
  "description": "Quick MARC editor",
  "main": "index.js",
  "repository": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.9.4"
  },
  "stripes": {
    "actsAs": [
      "plugin"
    ],
    "pluginType": "quick-marc",
    "displayName": "ui-quick-marc.meta.title",
    "okapiInterfaces": {
      "inventory": "10.0",
      "records-editor.records": "1.0"
    },
    "permissionSets": [
      {
        "permissionName": "ui-quick-marc.all",
        "displayName": "quickMARC: View, edit",
        "subPermissions": [
          "records-editor.records.item.get",
          "records-editor.records.item.put",
          "inventory.instances.item.get"
        ],
        "visible": true
      }
    ]
  },
  "scripts": {
    "build": "stripes build --output ./output",
    "lint": "eslint .",
    "start": "stripes serve",
    "test:unit": "jest --ci --coverage --passWithNoTests",
    "test:e2e": "stripes test karma --karma.singleRun --karma.browsers ChromeDocker --karma.reporters mocha junit --coverage",
    "test:e2e:dev": "stripes test karma --karma.browsers ChromeDocker",
    "test": "yarn run test:e2e && yarn run test:unit"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.7.4",
    "@bigtest/interactor": "^0.9.3",
    "@bigtest/mocha": "^0.5.1",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^5.0.0",
    "@folio/stripes": "^3.0.0",
    "@folio/stripes-cli": "^1.13.0",
    "@testing-library/react": "^9.3.2",
    "@welldone-software/why-did-you-render": "^4.0.5",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^25.1.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.1",
    "eslint": "^6.2.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jest": "^23.0.4",
    "faker": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-css-modules": "^2.1.0",
    "jest-junit": "^9.0.0",
    "miragejs": "^0.1.32",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-intl": "^2.5.0",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "@folio/stripes-acq-components": "^2.0.0",
    "final-form": "^4.18.2",
    "final-form-arrays": "^3.0.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.5.10",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.0",
    "react-final-form-listeners": "^1.0.2",
    "react-hot-loader": "^4.3.12",
    "react-router-prop-types": "^1.0.4",
    "uuid": "^3.0.1"
  },
  "peerDependencies": {
    "@folio/stripes": "^3.0.0",
    "react": "*",
    "react-intl": "^2.5.0",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0"
  }
}
