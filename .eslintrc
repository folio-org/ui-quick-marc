{
  "extends": ["@folio/eslint-config-stripes/acquisitions"],
  "parser": "@babel/eslint-parser",
  "overrides": [{
    "files": [ "*.test.js", "test/jest/**/*"],
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".jsx"]
        }
      }
    },
    "rules": {
      "no-unused-expressions": 0,
      "react/prop-types": "off",
      "func-names": "off",
      "filenames/match-exported": "off"
    }
  }, {
    "files": ["test/jest/**/*"],
    "rules": {
      "react/prop-types": "off",
    }
  }]
}
